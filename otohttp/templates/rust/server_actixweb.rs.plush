use actix_web::{get, web, App, HttpServer, Responder};
use crate::types;

#[derive(Error, Debug)]
pub enum Error {
    #[error("error: {0}")]
    MockError(String),
}

<%= for (service) in def.Services { %>



<%= for (method) in service.Unique { %>
